
<h2>Users</h2>

<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
  </thead>
</table>


<% @users.each do |user| %>
<div>
    <th><%= image_tag user.get_profile_image(100,100) %></th>
    <th><%= user.name %></th>
    <th><%= link_to "Show", user_path(user.id) %></th>
  </table>
</div>
</div>
<% end %>